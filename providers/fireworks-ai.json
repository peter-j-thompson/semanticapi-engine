{
    "provider": "fireworks-ai",
    "name": "Fireworks AI",
    "description": "Fast AI model inference platform optimized for speed",
    "base_url": "https://api.fireworks.ai/inference",
    "auth": {
        "type": "bearer",
        "header": "Authorization",
        "prefix": "Bearer"
    },
    "capabilities": [
        {
            "id": "chat",
            "name": "Chat Completion",
            "description": "Fast chat completion with open-source and fine-tuned models",
            "semantic_tags": [
                "chat",
                "completion",
                "fast",
                "llm",
                "inference"
            ],
            "endpoint": {
                "method": "POST",
                "path": "/v1/chat/completions",
                "params": {
                    "model": {
                        "type": "string",
                        "required": true,
                        "description": "Model ID"
                    },
                    "messages": {
                        "type": "array",
                        "required": true,
                        "description": "Chat messages"
                    },
                    "max_tokens": {
                        "type": "integer",
                        "required": false,
                        "description": "Max tokens"
                    }
                }
            }
        },
        {
            "id": "completion",
            "name": "Text Completion",
            "description": "Generate text completions optimized for speed",
            "semantic_tags": [
                "completion",
                "text",
                "fast",
                "generate",
                "inference"
            ],
            "endpoint": {
                "method": "POST",
                "path": "/v1/completions",
                "params": {
                    "model": {
                        "type": "string",
                        "required": true,
                        "description": "Model ID"
                    },
                    "prompt": {
                        "type": "string",
                        "required": true,
                        "description": "Text prompt"
                    }
                }
            }
        },
        {
            "id": "embeddings",
            "name": "Embeddings",
            "description": "Generate text embeddings for semantic search",
            "semantic_tags": [
                "embeddings",
                "vectors",
                "semantic",
                "encode",
                "text"
            ],
            "endpoint": {
                "method": "POST",
                "path": "/v1/embeddings",
                "params": {
                    "model": {
                        "type": "string",
                        "required": true,
                        "description": "Embedding model"
                    },
                    "input": {
                        "type": "string",
                        "required": true,
                        "description": "Text input"
                    }
                }
            }
        },
        {
            "id": "image",
            "name": "Image Generation",
            "description": "Generate images from text prompts using fast diffusion models",
            "semantic_tags": [
                "image",
                "generate",
                "diffusion",
                "creative",
                "fast"
            ],
            "endpoint": {
                "method": "POST",
                "path": "/v1/images/generations",
                "params": {
                    "model": {
                        "type": "string",
                        "required": true,
                        "description": "Image model"
                    },
                    "prompt": {
                        "type": "string",
                        "required": true,
                        "description": "Prompt"
                    }
                }
            }
        }
    ]
}